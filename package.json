{
  "name": "build-powershell",
  "version": "0.11.0",
  "description": "Atom Build provider for Microsoft PowerShell, runs PowerShell scripts",
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-build-powershell.git"
  },
  "license": "MIT",
  "keywords": [
    "buildprovider",
    "powershell",
    "ps1",
    "ps",
    "linter"
  ],
  "main": "lib/provider.js",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "providedServices": {
    "builder": {
      "description": "Runs PowerShell scripts",
      "versions": {
        "2.0.0": "provideBuilder"
      }
    }
  },
  "package-deps": [
    "build"
  ],
  "dependencies": {
    "atom-package-deps": "^4.6.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "coffeelint-stylish": "^0.1.2",
    "eslint": "^4.16.0",
    "eslint-config-atom-build": "^4.0.0",
    "gulp": "^4.0.0",
    "gulp-coffeelint": "^0.6.0",
    "gulp-debug": "^3.1.0",
    "gulp-eslint": "^4.0.1",
    "gulp-jsonlint": "^1.2.0",
    "gulp-lesshint": "^3.0.1",
    "jshint": "^2.9.4"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "atom-build",
    "rules": {
      "consistent-return": 0,
      "no-console": 0
    }
  },
  "scripts": {
    "test": "gulp lint"
  }
}
